sleep_ms(200)
gpio_put(25,1)
sleep_ms(300)
gpio_put(25,0)
sleep_ms(400)
gpio_put(25,1)
sleep_ms(500)
gpio_put(25,0)
sleep_ms(600)
gpio_put(25,1)
sleep_ms(700)
gpio_put(25,0)
sleep_ms(800)
gpio_put(25,1)
sleep_ms(900)
gpio_put(25,0)
sleep_ms(1000)
gpio_put(25,1)
sleep_ms(1100)
gpio_put(25,0)
sleep_ms(1200)
gpio_put(25,1)
sleep_ms(1300)
gpio_put(25,0)
sleep_ms(1400)
gpio_put(25,1)
sleep_ms(1500)
